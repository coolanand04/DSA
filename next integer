// "static void main" must be defined in a public class.
public class Main {
    public static void main(String[] args) {
    int a[] = {-5, -3, -1, 0, 1, 2, 4, 8, 9, 10};
    int x = 1;
    for(int i = 0; i < 10; i++)
    {
        
        if(x == a[i])
        {
            x++;
        }
       else
        {
           //System.out.print(x);
          
        }
        
    }
        System.out.print(x + " is the next integer required");

    }
}
